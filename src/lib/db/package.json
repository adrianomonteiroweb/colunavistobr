{
  "name": "@agendei/db",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./repositories/*": "./src/repositories/*.ts"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx ./scripts/seed.ts",
    "db:reset": "drizzle-kit drop && drizzle-kit migrate && tsx ./scripts/seed.ts"
  },
  "dependencies": {
    "@agendei/rrule": "workspace:*",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "pg": "^8.15.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.14",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.19.2"
  }
}
